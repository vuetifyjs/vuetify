{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": true,
  "rewrites": [
    {
      "source": "/eo-UY/(.*)",
      "destination": "/_crowdin"
    },
    {
      "source": "/(.*)",
      "destination": "$1"
    },
    {
      "source": "/(.*)",
      "destination": "/_fallback"
    }
  ],
  "headers": [
    {
      "source": "/service-worker.js",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "[a-z0-9]{8}\\.(js|css)(\\.map)?",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ],
  "github": {
    "enabled": false,
    "autoAlias": false,
    "silent": true
  }
}
