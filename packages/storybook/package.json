{
  "name": "storybook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "start-storybook -p 6006",
    "watch": "yarn start",
    "foo": "concurrently --no-color -n snapshot,e2e \"yarn test:snapshot\" \"yarn test:e2e\"",
    "test:snapshot": "cross-env BATCH=\"snapshot\" eyes-storybook",
    "test:e2e": "cross-env BATCH=\"e2e\" testcafe chrome:headless tests --app \"yarn start --ci --quiet\" --app-init-delay 15000"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@applitools/eyes-storybook": "^2.7.31",
    "@applitools/eyes-testcafe": "^1.1.20",
    "@babel/preset-react": "^7.0.0",
    "@mdi/font": "^3.9.96",
    "@storybook/addon-a11y": "^5.2",
    "@storybook/addon-actions": "^5.2",
    "@storybook/addon-knobs": "^5.2",
    "@storybook/addon-notes": "^5.2",
    "@storybook/addon-viewport": "^5.2",
    "@storybook/addons": "^5.2",
    "@storybook/vue": "^5.2",
    "@vue/cli-plugin-babel": "^4.0.0-beta.2",
    "@vue/cli-plugin-eslint": "^4.0.0-beta.2",
    "@vue/cli-service": "^4.0.0-beta.2",
    "babel-eslint": "^10.0.1",
    "babel-preset-vue": "^2.0.2",
    "concurrently": "^4.1.2",
    "cross-env": "^6.0.0",
    "deepmerge": "^4.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "js-beautify": "^1.10.2",
    "prism-react-renderer": "^0.1.7",
    "prismjs": "^1.17.1",
    "testcafe": "^1.3.3",
    "vue-storybook": "^1.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
