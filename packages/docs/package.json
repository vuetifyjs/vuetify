{
  "name": "vuetifyjs.com",
  "description": "A Vue.js project",
  "private": true,
  "author": "John Leider <john@vuetifyjs.com>",
  "version": "3.0.0-alpha.11",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuetifyjs/vuetify.git",
    "directory": "packages/docs"
  },
  "workspaces": {
    "nohoist": [
      "**/@vue/cli-service/**",
      "**/vue-server-renderer",
      "**/vue-template-compiler",
      "**/vue-cli-plugin-vuetify",
      "**/frontmatter-markdown-loader",
      "**/vuetify-loader"
    ]
  },
  "scripts": {
    "build": "yarn build:client",
    "dev": "yarn serve",
    "serve": "vue-cli-service serve",
    "lint": "yarn lint:md && vue-cli-service lint --no-fix",
    "lint:fix": "yarn fix:md && vue-cli-service lint",
    "build:client": "cross-env NODE_ENV=production vue-cli-service build",
    "build:server": "cross-env VUE_APP_WEBPACK_TARGET=node vue-cli-service build --no-clean",
    "build:modified": "node build/modified",
    "lint:md": "markdownlint --config .markdownlintrc src/pages/en",
    "fix:md": "markdownlint --config .markdownlintrc src/pages/en --fix",
    "prerender": "node build/prerender",
    "prod": "ws -z -d dist"
  },
  "dependencies": {
    "algoliasearch": "^4.8.5",
    "core-js": "^3.12.1",
    "node-fetch": "^2.6.0",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.12",
    "vue-i18n": "^8.17.7",
    "vue-instantsearch": "^3.4.3",
    "vue-router": "^3.1.6",
    "vuetify": "npm:vuetify@^2.5.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0-beta.1",
    "@vue/cli-plugin-eslint": "~5.0.0-beta.1",
    "@vue/cli-plugin-pwa": "~5.0.0-beta.1",
    "@vue/cli-plugin-router": "~5.0.0-beta.1",
    "@vue/cli-plugin-vuex": "~5.0.0-beta.1",
    "@vue/cli-service": "~5.0.0-beta.1",
    "@vuetify/api-generator": "^3.0.0-alpha.11",
    "@vuetify/vue-cli-plugin-base": "~0.3.5",
    "cosmicjs": "^3.2.43",
    "date-fns": "^2.14.0",
    "frontmatter-markdown-loader": "^3.6.2",
    "git-date-extractor": "^3.0.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "intersection-observer": "^0.11.0",
    "json-loader": "^0.5.7",
    "local-web-server": "^4.2.1",
    "lodash": "^4.17.19",
    "markdown-it": "^11.0.0",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-attrs": "^3.0.2",
    "markdown-it-header-sections": "^1.0.0",
    "markdown-it-prism": "^2.0.5",
    "markdownlint-cli": "^0.23.1",
    "marked": "^1.1.0",
    "minify-css-string": "^1.0.0",
    "mkdirp": "^1.0.4",
    "raw-loader": "^4.0.2",
    "sass": "~1.32",
    "sass-loader": "^12.1.0",
    "sitemap-webpack-plugin": "^1.1.0",
    "vee-validate": "^3.3.7",
    "vue-cli-plugin-vuetify": "^2.4.0",
    "vue-cookies": "^1.7.0",
    "vue-gtag": "^1.6.5",
    "vue-meta": "^2.4.0",
    "vue-prism-component": "^1.2.0",
    "vue-server-renderer": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "vuelidate": "^0.7.5",
    "vuetify-loader": "^1.7.2",
    "vuex": "^3.5.1",
    "vuex-pathify": "^1.4.1",
    "vuex-router-sync": "^5.0.0",
    "webfontloader": "^1.6.28",
    "webpack-bundle-analyzer": "^4.4.1",
    "workbox-precaching": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "vuePlugins": {
    "service": [
      "./plugins/@vuetify/ssg.js"
    ]
  }
}
