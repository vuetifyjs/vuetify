    "url-loader": "^0.6.2",
    "vue-color": "^2.4.5",
    "vue-loader": "^14.2.1",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.14",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  }
}
